<Loading>:
    BoxLayout:
        orientation: "vertical"
        canvas.before:
            Rectangle:
                id: png
                source: "ASSETS/Background.png"
                size: self.size
                pos: self.pos

        Image:
            id: png
            source: "ASSETS/LogoW.png"

            size_hint: 1, .3

        Image:
            id: zip
            source: "ASSETS/AirLoading.zip"
            anim_delay: .1
            anim_loop: 1

            size_hint: 1, .6

